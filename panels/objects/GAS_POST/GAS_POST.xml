  <script name="ScopeLib" isEscaped="1"><![CDATA[const string DP = $dp;

#property bool showbt
public void setShowbt(bool b){
  btn_fp.visible(b);
}
public bool getShowbt(){
  return btn_fp.visible;
}

void cbStatus(string dpe, bit32 state){
  rec_back.backCol = getBit(state, 3) ? &quot;&lt;{255,0,0},4,{0,0,0,0},4&gt;&quot; : &quot;_Transparent&quot;;
  rec_snd.backCol =  getBit(state, 4) ? &quot;red&quot; : &quot;WF_Border&quot;;
  rec_10.backCol =   getBit(state, 9) ? &quot;red&quot; : &quot;WF_Border&quot;;
  rec_30.backCol =   getBit(state, 10)? &quot;red&quot; : &quot;WF_Border&quot;;
  txt_test.visible = getBit(state, 12);
  txt_maint.visible = getBit(state, 13);
  line_snd_off.visible = getBit(state, 14);
}

void cbPorog(dyn_string dpe, dyn_float val) {
  txt_10.text(val[1] + &quot;%&quot;);
  txt_30.text(val[2] + &quot;%&quot;);
}
]]></script>
  <script name="Initialize" isEscaped="1"><![CDATA[main(){
  btn_fp.backCol(&quot;_Transparent&quot;);
  txt_comment.text = dpGetAlias(DP + &quot;.&quot;);
  dpConnect(&quot;cbStatus&quot;, DP + &quot;.status.sts_bit&quot;);
  dpConnect(&quot;cbPorog&quot;, DP + &quot;.config.val_h&quot;,
                       DP + &quot;.config.val_hh&quot;);
}
]]></script>
